        <arguments>
          <argument>
            <kind>1</kind>
            <string>///move to mouse, click
x = mouse_x;
y = mouse_y;
if global.color == true
{
    image_alpha = 255
}
else
{
    image_alpha = 0
}
//reset poi
if place_meeting(x,y, obj_interest) &amp;&amp; click = false
{
    poi = 1
    sprite_index = spr_open
}
else if place_meeting(x,y, obj_interest) &amp;&amp; click = true
{
    poi = 1
    sprite_index = spr_curser_closed
}
else if place_meeting(x,y, obj_blind2) &amp;&amp; click = false  &amp;&amp; steam_get_achievement("ACHIEVEMENT_FINISH")
{
    poi = 1
    sprite_index = spr_open
}
else if place_meeting(x,y, obj_blind2) &amp;&amp; click = true &amp;&amp; steam_get_achievement("ACHIEVEMENT_FINISH")
{
    poi = 1
    sprite_index = spr_curser_closed
}
else
{
    poi = 0
    image_speed = 0.10
}
//pick symbol
if global.symbol == 'hand' &amp;&amp; poi == 0
{
    sprite_index = spr_curser;
} 
else if global.symbol == 'mash' &amp;&amp; poi == 0
{
    sprite_index = spr_curser_mash;
} 
else if global.symbol == 'eye' &amp;&amp; poi == 0
{
    sprite_index = spr_curser_eye;
} 
//sound_stop_all()
</string>
          </argument>
        </arguments>
      </action>
